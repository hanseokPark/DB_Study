<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kr.or.dgit.it.datapersistenceapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".FileIOActivity" />
        <activity android:name=".ReadFileActivity" />
        <activity android:name=".SharedPrefActivity" />
        <activity android:name=".SharedPrefFragmentActivity" />
        <activity android:name=".SpannableActivity" />
        <activity android:name=".WebViewActivity" />
        <activity android:name=".SpinnerActivity" />
        <activity android:name=".WidgetActivity" />
        <activity
            android:name=".ActionBarActivity"
            android:label="@string/title_activity_action_bar"
            android:theme="@style/MainTheme"></activity>
    </application>

</manifest>